{
  "name": "typeofweb-bundlephobia-pr-stats-action",
  "version": "0.0.1",
  "main": "dist/main.js",
  "repository": "https://github.com/typeofweb/typeofweb-bundlephobia-pr-stats-action",
  "author": "Micha≈Ç Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node12": "1.0.7",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "@vercel/ncc": "0.27.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-functional": "3.2.1",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@actions/artifact": "0.5.0",
    "@actions/cache": "1.0.5",
    "@actions/core": "1.2.6",
    "@actions/github": "4.0.0",
    "gzip-size": "6.0.0",
    "pretty-bytes": "5.5.0"
  },
  "scripts": {
    "build": "NODE_ENV=production yarn ncc build src/main.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "yarn prettier --write"
    ],
    "*.{json,md,yaml,yml}": [
      "yarn prettier --write"
    ]
  }
}
